{
    "name": "fynduck/laravel-searchable",
    "description": "A description for laravel-searchable.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "fynduck",
            "email": "stason.k12@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^5|^6",
        "laravel/framework": "^5|^6",
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Fynduck\\LaravelSearchable\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fynduck\\LaravelSearchable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fynduck\\LaravelSearchable\\LaravelSearchableServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.6"
    }
}
